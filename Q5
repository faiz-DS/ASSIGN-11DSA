def find_intersection(nums1, nums2):
    set1 = set(nums1)
    set2 = set(nums2)
    intersection = set1.intersection(set2)
    return list(intersection)

# Test cases
print(find_intersection([1, 2, 2, 1], [2, 2]))  # Output: [2]
print(find_intersection([4, 9, 5], [9, 4, 9, 8, 4]))  # Output: [9, 4]
